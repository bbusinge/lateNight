<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.businge.iNavigate">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="iNavigate"
        android:roundIcon="@mipmap/ic_launcher_new_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity2">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
<!--                change the action & category -->
<!--                for the action tag, use the package name.activity name-->
<!--                this is how you customise your action view-->
<!--                also on the category tag, replace Launcher with default-->
                <action android:name="com.businge.iNavigate.MainActivity2" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>


    </application>

</manifest>